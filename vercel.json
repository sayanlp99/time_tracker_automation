{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "api/index.js"
    },
    {
      "src": "/heartbeat",
      "methods": ["GET"],
      "dest": "api/index.js"
    }
  ],
  "buildCommand": "npm install"
}
